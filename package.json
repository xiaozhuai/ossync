{
    "name": "ossync",
    "version": "1.0.1",
    "description": "aliyun oss sync tool, like rsync",
    "main": "index.js",
    "scripts": {
        "build-macos64": "pkg -t node18-macos-x64 -o dist/ossync_macos64 index.js",
        "build-linux64": "pkg -t node18-linux-x64 -o dist/ossync_linux64 index.js",
        "build-win64": "pkg -t node18-win-x64 -o dist/ossync64.exe index.js"
    },
    "repository": "https://github.com/xiaozhuai/ossync",
    "author": "xiaozhuai",
    "license": "MIT",
    "dependencies": {
        "ali-oss": "^6.17.1",
        "chalk": "^5.3.0",
        "cli-progress": "^3.12.0",
        "ignore": "^5.2.4",
        "sade": "^1.8.1",
        "xxhashjs": "^0.2.2"
    },
    "devDependencies": {
        "@types/ali-oss": "^6.16.8",
        "@types/xxhashjs": "^0.2.2",
        "@types/node": "^18.17.0",
        "pkg": "^5.8.1"
    }
}
